* Match statements
let(<x>) -> 42
match(x) -> {
	case(42) -> {
		print("tis the number 42.")
	}
	case(128) -> {
		print("ooh, an multiplee of 8!")
	}
	case(null) -> {
		print("crap, a nothin!")
	}
	default -> {
		print("~not nothing~")
	}
}