<!turtle>
<!web>

fn(<img>) -> {
	obj(<el>) -> web.make("img")
	el.setAttr("src", param(0))
	web.body.add(el)
	el.on("load") -> {
		yield()
	}
}

img("https://mageowlstudios.com/images/owl.png") -> {
	print(param(0))
}

obj(<canvas>) -> web.make("canvas")
web.body.add(canvas)

obj(<turtle>) -> turtle.new(canvas)

turtle.goto(50, 50)
turtle.size(10) turtle.cap("round") turtle.join("round")
turtle.start("fill")

repeat(4) -> {
	turtle.forward(70)
	turtle.right(90)
}

turtle.end()