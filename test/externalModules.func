<!turtle>
<!web>
<!meta>

fn(<img>) -> {
	obj(<el>) -> web.make("img")
	el.setAttr("src", param(0))
	el.setAttr("style", "display:none;")
	web.body.add(el)
	el.on("load") -> {
		yield(el)
	}
}

obj(<canvas>) -> web.make("canvas")
web.body.add(canvas)

obj(<turtle>) -> turtle.new(canvas)

turtle.goto(50, 50)
turtle.size(10) turtle.cap("square") turtle.join("miter")
turtle.start("fill")

repeat(4) -> {
	turtle.forward(70)
	turtle.right(90)
}

turtle.end()

img("https://mageowlstudios.com/images/gradient.png") -> {
	turtle.image(param(0))
}
