<!mageowl.turtle>
<!web>
<!meta>

fn(<img>) -> {
	let(<el>) -> web.make("img")
	el.setAttr("src", param(0))
	el.setAttr("style", "display:none;")
	web.body.add(el)
	el.on("load") -> {
		yield(el)
	}
}

let(<canvas>) -> web.make("canvas")
web.body.add(canvas)

let(<t1>) -> turtle.new(canvas)

t1.goto(50, 50)
t1.size(10) t1.cap("square") t1.join("miter")
t1.start("fill")

repeat(4) -> {
	t1.forward(70)
	t1.right(90)
}

t1.end()

img("https://mageowlstudios.com/images/gradient.png") -> {
	t1.image(param(0))
}
